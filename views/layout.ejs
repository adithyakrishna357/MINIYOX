                <!-- add to cart -->
                <script>
                    function addTocart(proId) {
                        $.ajax({
                            url: "/addtocart/" + proId,
                            method: "get",
                            success: (response) => {
                                console.log(response);
                                if (response.status) {
                                    let count = response.cartCount;
                                    let cart = document.querySelector('.js-show-cart');
                                    cart.setAttribute('data-notify', response.cartCount);
                                    Toastify({
                                        text: "Product added to cart!",
                                        duration: 3000,
                                        newWindow: true,
                                        gravity: "top", // top or bottom
                                        position: 'center', // left, center or right
                                        backgroundColor: "linear-gradient(to right, #717fe0, #000000)",
                                    }).showToast();

                                }
                            }
                        })
                    }
                </script>
